{
  "app": {
    "bundleName": "com.wishfm.tvapp",
    "vendor": "BMPI",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    }
  },
  "deviceConfig": {
    "default": {}
  },
  "module": {
    "package": "com.wishfm.tvapp.fascheduler",
    "name": ".MyApplication",
    "deviceType": [
      "phone",
      "tablet",
      "tv"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "fascheduler",
      "moduleType": "feature",
      "installationFree": true
    },
    "abilities": [
      {
        "orientation": "unspecified",
        "visible": true,
        "name": "com.wishfm.tvapp.fascheduler.MainAbility",
        "icon": "$media:icon",
        "description": "$string:mainability_description",
        "label": "$string:app_name",
        "type": "page",
        "launchType": "standard",
        "metaData": {
          "customizeData": [
            {
              "name": "hwc-theme",
              "value": "androidhwext:style/Theme.Emui.Translucent.NoTitleBar"
            }
          ]
        },
        "skills": [
          {
            "actions": [
              "action.fascheduler.DeviceListDialogSlice",
              "action.fascheduler.OpenApkDialogSlice"
            ]
          }
        ]
      }
    ],
    "reqPermissions": [
      {
        "name": "ohos.permission.LOCATION"
      },
      {
        "name": "ohos.permission.LOCATION_IN_BACKGROUND"
      },
      {
        "name": "ohos.permission.servicebus.ACCESS_SERVICE"
      },
      {
        "name": "ohos.permission.DISTRIBUTED_DEVICE_STATE_CHANGE"
      },
      {
        "name": "ohos.permission.GET_DISTRIBUTED_DEVICE_INFO"
      },
      {
        "name": "ohos.permission.GET_BUNDLE_INFO"
      },
      {
        "name": "ohos.permission.DISTRIBUTED_DATASYNC"
      },
      {
        "name": "ohos.permission.SYSTEM_FLOAT_WINDOW"
      },
      {
        "name": "ohos.permission.INTERNET"
      },
      {
        "name": "android.permission.INTERNET"
      },
      {
        "name": "ohos.permission.GET_BUNDLE_INFO_PRIVILEGED"
      }
    ]
  }
}